    <xsd:attribute name="version" type="VersionString" default="2.1">
     <xsd:annotation>
      <xsd:documentation>Version number of response. Fixed.</xsd:documentation>
     </xsd:annotation>
    </xsd:attribute>
   </xsd:extension>
  </xsd:complexContent>
 </xsd:complexType>
