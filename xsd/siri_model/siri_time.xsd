		<xsd:annotation>
			<xsd:documentation>Subset of TPEG Pti34 - DayType</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="DayTypeEnumeration">
			<xsd:enumeration value="holiday"/>
