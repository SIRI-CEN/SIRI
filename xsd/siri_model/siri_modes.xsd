			</xsd:enumeration>
			<xsd:enumeration value="telecabinService">
				<xsd:annotation>
					<xsd:documentation>See also 'telecabin'.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="taxi"/>
			<xsd:enumeration value="selfDrive"/>
			<xsd:enumeration value="all"/>
