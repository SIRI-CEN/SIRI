	<xsd:element name="VehicleMode" type="VehicleModesOfTransportEnumeration" default="unknown">
		<xsd:annotation>
			<xsd:documentation>Vehicle mode or mode of transport.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
